<div>
  <h1>ログイン</h1>
</div>


    <%= form_with(url: login_path, scope: :session) do |f| %>

      <div>
        <%= f.label :email, 'メールアドレス' %>
        <%= f.email_field :email %>
      </div>

      <div>
        <%= f.label :password, 'パスワード' %>
        <%= f.password_field :password %>
      </div>

      <%= f.submit 'ログイン' %>
    <% end %>

    <p>会員登録は<%= link_to 'こちら', signup_path %></p>
  </div>
</div>